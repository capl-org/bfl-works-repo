# symfony default
/.web-server-pid
/app/config/parameters.yml
/build/
/phpunit.xml
/web/bundles/

 

# local config
/.env.local
/.env.local.php
/.env.*.local
!/app/config/local
/app/config/local/*
!app/config/local/.gitkeep

 

# pimcore legacy (remove this for your own development)
!/legacy
/legacy/*
!legacy/.gitkeep
!legacy/bundle

 

/var/*
